{
  "schemaVersion": 1,
  "id": "common-cfg",
  "version": "1.0.0",
  "name": "Common Config",
  "description": "Config and debug console for therealfarfetchd's mods.",
  "authors": ["the_real_farfetchd"],
  "contact": {
    "homepage": "https://github.com/qcommon/cfg",
    "sources": "https://github.com/qcommon/cfg"
  },
  "license": "MIT",
  "icon": "assets/common-cfg/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{ "value": "therealfarfetchd.commoncfg.CommonCfg", "adapter": "kotlin" }],
    "client": [{ "value": "therealfarfetchd.commoncfg.CommonCfgClient", "adapter": "kotlin" }],
    "commands": ["therealfarfetchd.commoncfg.CommandInit"],
    "commands_client": ["therealfarfetchd.commoncfg.CommandInitClient"]
  },
  "mixins": [
    "commoncfg.mixins.json"
  ],
  "requires": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.3.30"
  }
}
